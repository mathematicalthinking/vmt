{
  "name": "vmt",
  "version": "2.1.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-production": "NODE_ENV=production node ./bin/www",
    "start-staging": "NODE_ENV=staging node ./bin/www",
    "dev": "NODE_ENV=development concurrently \"nodemon --ignore '../client'\" \"npm run client\" \"redis-server\"",
    "dev:pc": "\"SET NODE_ENV=development&& concurrently \"nodemon --ignore '../client'\" \"npm run client\"",
    "client": "cd ../client && npm run start",
    "build-staging": "cd ../client && node --max-old-space-size=3072 scripts/build.js staging",
    "build-production": "cd ../client && node --max-old-space-size=3072 scripts/build.js production",
    "build-production-test": "cd ../client && node scripts/build.js test",
    "build-enc-dev": "cd ../client && node scripts/build.js dev encompass",
    "build-enc-staging": "cd ../client && node scripts/build.js staging encompass",
    "build-enc-production": "cd ../client && node scripts/build.js production encompass",
    "build-enc-test": "cd ../client && node scripts/build.js test encompass",
    "test": "NODE_ENV=test REACT_APP_IS_TEST=true concurrently \"nodemon\" \"npm run client\" \"npm run cypress\"",
    "test:pc": "\"SET NODE_ENV=test&& SET REACT_APP_IS_TEST=true&& concurrently nodemon \"npm run client\" \"npm run cypress:pc\"",
    "testserver:pc": "\"SET NODE_ENV=test&& SET REACT_APP_IS_TEST=true&& concurrently nodemon \"npm run cypress:pc\"",
    "prodTest": "NODE_ENV=production && node ./bin/www",
    "testEncompass": "NODE_ENV=production && ENCOMPASS=true && node ./bin/www",
    "cypress": "./node_modules/.bin/cypress open",
    "cypress:pc": ".\\node_modules\\.bin\\cypress open",
    "cypress:run": "cypress run --browser electron",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "heroku-postbuild": "cd ../client && npm install && npm run build",
    "seed": "node cypress/seedDb",
    "seed-sso": "cd ../../mt-sso && npm run test",
    "seed-enc": "cd ../../encompass &&  grunt serve-seed",
    "test-vmt": "concurrently \"npm:test\"  \"npm:seed-sso\" \"redis-server\"",
    "test-all": "concurrently \"npm:seed-enc\" \"npm:test\"  \"npm:seed-sso\" \"redis-server\"",
    "restore-sso": "cd ../../mt-sso && npm run seed",
    "restore-enc": "cd ..",
    "dev-sso": "cd ../../mt-sso && npm run dev",
    "dev-enc": "cd ../../encompass && npm run dev",
    "dev-all": "concurrently \"npm:dev\" \"npm:dev-sso\" \"npm:dev-enc\"",
    "dev-vmt": "concurrently \"npm:dev\" \"npm:dev-sso\"",
    "test-travis": "NODE_ENV=test REACT_APP_IS_TEST=true npm run start",
    "cypress:record": "cypress run --browser electron --record --key 103dfac6-8e4d-4672-adf9-b661fb7738da",
    "test-start": "NODE_ENV=test concurrently \"nodemon --ignore '../client'\" \"npm run client\""
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
    "axios": "^0.25.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.10",
    "express": "^4.17.3",
    "express-handlebars": "^5.3.1",
    "express-prom-bundle": "^6.4.1",
    "human-readable-ids": "^1.0.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^5.12.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "prom-client": "^13.2.0",
    "serve-favicon": "~2.4.5",
    "socket.io": "^4.5.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cypress": "^9.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^2.0.19",
    "onchange": "^7.1.0",
    "prettier": "^1.16.3"
  }
}
