{
  "name": "vmt",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-production": "NODE_ENV=production node ./bin/www",
    "start-staging": "NODE_ENV=staging node ./bin/www",
    "dev": "NODE_ENV=dev concurrently \"nodemon --ignore '../client'\" \"npm run client\"",
    "client": "cd ../client && npm run start",
    "build-staging": "cd ../client && node scripts/build.js staging",
    "build-production": "cd ../client && node scripts/build.js production vmt",
    "build-production-test": "cd ../client && node scripts/build.js test vmt",
    "build-enc-dev": "cd ../client && node scripts/build.js dev encompass",
    "build-enc-staging": "cd ../client && node scripts/build.js staging encompass",
    "build-enc-production": "cd ../client && node scripts/build.js production encompass",
    "build-enc-test": "cd ../client && node scripts/build.js test encompass",
    "test": "NODE_ENV=test REACT_APP_IS_TEST=true concurrently \"nodemon\" \"npm run client\" \"npm run cypress\"",
    "prodTest": "SET \"NODE_ENV=production\" && node ./bin/www",
    "testEncompass": "SET \"NODE_ENV=production\" && SET \"ENCOMPASS=true\" && node ./bin/www",
    "cypress": "./node_modules/.bin/cypress open",
    "cypress:run": "cypress run --browser electron",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "heroku-postbuild": "cd ../client && npm install && npm run build",
    "seed": "node cypress/seedDb",
    "seed-sso": "cd ../mt-sso && npm run test",
    "seed-enc": "cd ../encompass &&  grunt serve-seed",
    "test-all": "concurrently \"npm:seed-enc\" \"npm:test\"  \"npm:seed-sso\"",
    "restore-sso": "cd ../../mt-sso && npm run seed",
    "restore-enc": "cd ../../encompass && npm run seed",
    "dev-sso": "cd ../../mt-sso && npm run dev",
    "dev-enc": "cd ../../encompass && npm run start",
    "dev-all": "concurrently \"npm:dev\" \"npm:dev-sso\" \"npm:dev-enc\"",
    "test-travis": "NODE_ENV=test REACT_APP_IS_TEST=true npm run start",
    "cypress:record": "cypress run --browser electron --record --key 103dfac6-8e4d-4672-adf9-b661fb7738da",
    "test-start": "NODE_ENV=test concurrently \"nodemon --ignore '../client'\" \"npm run client\""

  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.10",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.4.17",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.1.1",
    "xml2js": "^0.4.19",
    "cypress-file-upload": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.18.10",
    "onchange": "^5.2.0",
    "prettier": "^1.16.3"
  }
}
